@model IEnumerable<CaxSecurity.Models.Reportes>

@{
    ViewData["Title"] = "Bienvenidos";
}

<div class="container">
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Descripción reporte</th>
                    <th scope="col">Nombre Barrio</th>
                    <th scope="col">Tipo Reporte</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.descripcionReporte</td>
                        <td>@item.nombreBarrio</td>
                        <td>@item.tipoReporte</td>
                    </tr>
                }                
            </tbody>
        </table>
    </div>
</div>
